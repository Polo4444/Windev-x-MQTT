      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      procédure AfficheUtilisateurStatus() <UI>
      
      FEN_Chat.ZR_Utilisateurs.Affiche(taCourantBandeau)
     type : 458752
   -
     name : ObtenirCouleurStatus
     procedure_id : 1517771881125177632
     type_code : 14
     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //[ <Résultat> = ] ObtenirCouleurStatus (<nStatus> est entier)
      //
      // Paramètres :
      //	nStatus (entier) : <indiquez ici le rôle de nStatus>
      // Valeur de retour :
      // 	Couleur : // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      procédure ObtenirCouleurStatus(nStatus est un entier) :  Couleur
      
      cCouleur est une Couleur
      selon nStatus
      	
      	cas -1:
      		cCouleur =  HTMLVersRVB("#616161")	// Gris
      	
      	cas 0:
      		cCouleur = HTMLVersRVB("#F2A600")	// Orange
      		
      	CAS 1:
      		cCouleur = HTMLVersRVB("#0D904F")	// Vert
      	
      	AUTRE CAS
      		cCouleur =  HTMLVersRVB("#616161")	// Hors ligne par défaut
      FIN
      
      renvoyer cCouleur
      
     type : 458752
