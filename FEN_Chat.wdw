  p_codes :
   -
     code : |1+
      PROCEDURE MaFenêtre()
      
      STUtilisateurAdapter est une structure
      	Utilisateur est un CUtilisateur
      	
      	// Status assigne une couleur en fonction de la présence de l'utilisateur
      	Status est une Couleur	
      fin
      
      // Liste de messages
      gtabMessages est un tableau de CMessage
      
      // Liste des utilisateurs
      gtabUtilisateursAdapter est un tableau de STUtilisateurAdapter
      
      // On lance les écoutes de messages
      COL_Com.EcouterMessages(COL_Com.FORUM_TOPIC, EcouterMessagesForum)
      COL_Com.EcouterStatus(COL_Com.STATUS_TOPIC, EcouterStatusUtilisateur)
   -
     code : |1+
      
      // On demande à l'utilisateur de saisir son nom
      // Tant que l'utilisateur n'a pas défini son nom, on lui repose la même question
      sNom est une chaine = ""
      TANTQUE sNom = ""
      	SELON Saisie(<§?151002F6002E8AB3§>, sNom)
      		CAS 1 :
      			SI SansEspace(sNom) = "" ALORS
      				sNom = ""
      				CONTINUER
      			FIN
      			gclMyUser.Nom = sNom
      			MaFenêtre..Titre = "Chat - " + sNom
      	FIN
      FIN
      
      sNomFI est une chaine = ""
      pour tout clMes de gtabMessages
      	
      	si clMes.Auteur._id = gclMyUser._id ALORS
      		sNomFI = "FI_Chat_Moi"
      	sinon
      		sNomFI = "FI_Chat_Autre"
      	FIN
      	ZR_Chat.AjouteFI(sNomFI, clMes)
      FIN
      
      // On signale son status
      gclMyUser.Status = CUtilisateur.StatusEnLigne()
      gclMyUser.PublierStatus()
      
      // On demande les status existants
      COL_Com.PublierDemandeStatusExistants()
     type : 34
   -
     type : 230
     enabled : false
  procedures :
   -
     name : EcouterMessagesForum
     procedure_id : 1517714418749194440
     type_code : 14
     code : |1-
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //EcouterMessagesForum (<sTopic> est chaîne, <clMes> est CMessage)
      //
      // Paramètres :
      //	sTopic (chaîne ANSI) : <indiquez ici le rôle de sTopic>
      //	clMes (CMessage) : <indiquez ici le rôle de clMessage>
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      procédure EcouterMessagesForum(sTopic est une chaîne, clMes est un CMessage)
      
      // On s'assure d'être sur le bon topic
      si sTopic <> COL_Com.FORUM_TOPIC ALORS
      	retour
      FIN
      
      // Si je suis l'auteur du message, on ne fait rien
      si clMes.Auteur._id = gclMyUser._id ALORS
      	retour
      FIN
      
      // On ajoute le messages à la liste des messages et à la zone répétée
      gtabMessages.Ajoute(clMes)
      ExécuteThreadPrincipal(AfficheMessageAutre, clMes)
     type : 458752
   -
     name : AfficheMessageAutre
     procedure_id : 1517743701839825258
